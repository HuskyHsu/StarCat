{
    "manifest_version": 3,
    "name": "Github Stars Categorize",
    "version": "1.0",
    "description": "Automatically categorize and list your Github stars for easy browsing.",
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "192": "images/icon192.png",
        "512": "images/icon512.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://github.com/*"
            ],
            "js": [
                "scripts/content.js"
            ]
        }
    ]
}